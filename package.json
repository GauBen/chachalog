{
	"name": "chachalog",
	"version": "0.3.9",
	"repository": {
		"url": "https://github.com/GauBen/chachalog"
	},
	"license": "AGPL-3.0-only",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./yarn": {
			"types": "./dist/yarn.d.ts",
			"import": "./dist/yarn.js"
		},
		"./github": {
			"types": "./dist/github.d.ts",
			"import": "./dist/github.js"
		},
		"./pnpm": {
			"types": "./dist/pnpm.d.ts",
			"import": "./dist/pnpm.js"
		},
		"./package.json": "./package.json"
	},
	"bin": "dist/bin.js",
	"files": [
		"dist"
	],
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"build": "tsc && pkgroll --clean-dist --minify && dts-buddy packages/types/index.d.ts && publint",
		"check": "biome check --fix",
		"format": "biome format --write",
		"test": "node --experimental-test-coverage --test '**/*.test.ts'"
	},
	"devDependencies": {
		"@actions/github": "^6.0.0",
		"@biomejs/biome": "^2.0.0-beta.1",
		"@clack/prompts": "^0.10.0",
		"@pnpm/logger": "^1000.0.0",
		"@pnpm/workspace.find-packages": "^1000.0.19",
		"@pnpm/workspace.read-manifest": "^1000.1.2",
		"@types/mdast": "^4.0.4",
		"@types/node": "^20.17.30",
		"@types/semver": "^7.7.0",
		"@types/unist": "^3.0.3",
		"@yarnpkg/core": "^4.3.1",
		"@yarnpkg/fslib": "^3.1.2",
		"clipanion": "^4.0.0-rc.4",
		"dts-buddy": "^0.6.0",
		"pkgroll": "^2.12.1",
		"publint": "^0.3.10",
		"remark": "^15.0.1",
		"remark-frontmatter": "^5.0.0",
		"semver": "^7.7.1",
		"tinyglobby": "^0.2.12",
		"typescript": "^5.8.3",
		"vfile-matter": "^5.0.1",
		"yaml": "^2.7.1"
	},
	"packageManager": "yarn@4.8.1"
}
